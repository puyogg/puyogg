{
  "name": "@puyogg/infra-shared",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/puyogg/puyogg.git",
    "directory": "infrastructure/shared"
  },
  "scripts": {
    "clean": "rm -f tsconfig.tsbuildinfo && rm -rf esm",
    "format": "prettier --write ./src",
    "format-check": "prettier --check ./src",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "tsc:esm": "tsc",
    "typecheck": "tsc",
    "babel:esm": "BABEL_ENV=esm babel src --out-dir esm --extensions '.ts'",
    "build:esm": "pnpm tsc:esm && pnpm babel:esm",
    "watch:esm": "concurrently \"pnpm tsc:esm --watch --preserveWatchOutput\" \"pnpm babel:esm --watch\"",
    "build": "pnpm build:esm"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/babel__core": "^7.20.4",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@typescript-eslint/utils": "^6.11.0",
    "babel-plugin-source-map-support": "^2.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@pulumi/aws": "^6.17.0",
    "@pulumi/awsx": "^2.4.0",
    "@pulumi/pulumi": "^3.99.0",
    "@puyogg/infra-components": "workspace:^"
  }
}
